{
    "extends": ["next/core-web-vitals", "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "array-bracket-spacing": [1, "always"],
        "no-tabs": 0,
        "no-unused-vars": 0,
        "prefer-const": 0,
        "require-await": 1,
        "prefer-promise-reject-errors": 1,
        "no-extend-native": 2,
        "no-else-return": 1,
        "default-case": 1,
        "class-methods-use-this": 1,
        "react/jsx-curly-spacing": [1, { "when": "never" }],
        "react/no-array-index-key": 1,
        "@next/next/no-sync-scripts": 0
    }
}